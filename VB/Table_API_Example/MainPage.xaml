                        <dxb:BarCheckItemLink BarItemName="biTableToggleTopBorder" />
                        <dxb:BarCheckItemLink BarItemName="biTableToggleLeftBorder" />
                        <dxb:BarCheckItemLink BarItemName="biTableToggleRightBorder" />
